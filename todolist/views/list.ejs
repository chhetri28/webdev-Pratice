<%- include("headers") -%>
    <div class="box" id="heading">
        <h1> <%= ListTitle %> </h1>
    </div>
    <div class="box">

        <% for(let i=0;i<newitems.length ;i++){ %>
            <div class="item">
                <input type="checkbox">
                <p>
                    <%= newitems[i] %>
                </p>
            </div>
            <% } %>
                <form class="item" action="/" method="post">
                    <input type="text" name="nextname" placeholder="New Item" autocomplete="off">
                    <button type="submit" name="list" value=<%=ListTitle %>  >+</button>
                </form>
    </div>
<%- include("footer") -%>
